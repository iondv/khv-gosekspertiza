		</xs:annotation>
		<xs:sequence>
			<xs:element name="EngineeringSurveyMismatch" type="tMismatch" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о несоответствии проектной документации результату инженерных изысканий</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ProjectTaskMismatch" type="tMismatch" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Сведения о несоответствии проектной документации заданию на проектирование</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="NormsMismatch" type="tMismatch" minOccurs="0" maxOccurs="unbounded">
