			<xs:element name="DocIssueAuthor" type="string1000" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Организации, ИП или физическое лицо - автор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="FullDocIssueAuthor" type="tDeclarant" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Организации, ИП или физическое лицо - автор документа</xs:documentation>
				</xs:annotation>
			</xs:element>
			</xs:choice>
			<xs:element name="DocChanges" type="string50" minOccurs="0" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Примечание об изменении документа</xs:documentation>
				</xs:annotation>
			</xs:element>

			<xs:choice minOccurs="1" maxOccurs="1">
			<xs:element name="File" type="tFile" minOccurs="1" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Файл документа или файл фрагмента документа, поданного для проведения экспертизы</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Included" type="string1000" minOccurs="1" maxOccurs="1">
				<xs:annotation>
