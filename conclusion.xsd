	<!-- Определение простых типов -->
	<xs:simpleType name="tFileFormat">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 4 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFileChecksum">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 8 символов</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9a-fA-F]{8}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string50">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 50 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string100">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 100 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string200">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 200 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="200"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string500">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 500 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="500"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="string1000">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной 1000 символа</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="1000"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tCadastralNumber">
		<xs:annotation>
			<xs:documentation>Кадастровый номер участка</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="\d+:\d+:\d+:\d+"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFileName">
		<xs:annotation>
			<xs:documentation>Текстовая строка длиной до 255 символов - имя файла</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:maxLength value="255"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tEmail">
		<xs:annotation>
			<xs:documentation>Текстовая строка - адрес электронной почты</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="[a-zA-Zа-яА-Я0-9_.\-]{1,}[@]{1}[a-zA-Zа-яА-Я0-9_.\-]{1,}[.]{1}[a-zA-Zа-яА-Я]{2,}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFunctionsClass">
		<xs:annotation>
			<xs:documentation>Код классификатора функционального назначения объектов капитального строительства</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}\.[0-9]{1,3})|([0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3})"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tFunctionsClassFull">
		<xs:annotation>
			<xs:documentation>Код классификатора функционального назначения объектов капитального строительства</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{1,2}\.[0-9]{1,2}\.[0-9]{1,3}\.[0-9]{1,3}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tConclusionNumber">
		<xs:annotation>
			<xs:documentation>Номер заключения экспертизы (старый формат или ЕГРЗ)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]{2}-[1-2]{1}-[1-2]{1}-[1-3]{1}-[0-9]{6}-[0-9]{4})|([0-9]{2}-[1-2]{1}-[1-2]{1}-[1-3]{1}-[0-9]{4}-[0-9]{2})" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tSimpleConclusionNumber">
		<xs:annotation>
			<xs:documentation>Номер заключения по результатам оценки соответствия в рамках экспертного сопровождения</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]{4}-[0-9]{4}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tConclusionGUID">
		<xs:annotation>
			<xs:documentation>Уникальный идентификатор формирования заключения</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{4}-[0-9A-Fa-f]{12}" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tSchemaVersion">
		<xs:annotation>
			<xs:documentation>Номер версии, использованной при формировании заключения в формате xx.xx</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="([0-9]){2}.([0-9]){2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tExpertCertificate">
		<xs:annotation>
			<xs:documentation>Номер квалификационного аттестата эксперта </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:pattern value="(МС-Э-[0-9]{1,3}-[0-9]{1,2}-[0-9]{4,5})|(ГС-Э-[0-9]{1,2}-[0-9]{1,2}-[0-9]{4,5})" />
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tPostIndex">
		<xs:annotation>
			<xs:documentation>Почтовый индекс</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:length value="6"/>
			<xs:pattern value="\d{6}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tOrgOGRN">
		<xs:annotation>
			<xs:documentation>ОГРН организации</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:length value="13"/>
			<xs:pattern value="\d{13}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tOrgINN">
		<xs:annotation>
			<xs:documentation>ИНН организации</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:length value="10"/>
			<xs:pattern value="\d{10}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tOrgKPP">
		<xs:annotation>
			<xs:documentation>КПП организации</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:length value="9"/>
			<xs:pattern value="\d{9}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tOGRNIP">
		<xs:annotation>
			<xs:documentation>ОГРНИП</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:normalizedString">
			<xs:length value="15"/>
			<xs:pattern value="\d{15}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="tSNILS">
		<xs:annotation>
			<xs:documentation>СНИЛС</xs:documentation>
